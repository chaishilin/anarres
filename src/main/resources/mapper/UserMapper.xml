<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csl.anarres.mapper.UserMapper">

    <sql id="userColumnAll">
        ID,USERNAME,CREATETIME,LASTMODIFIEDTIME
    </sql>
    <select id="userInfoList" resultType="com.csl.anarres.entity.UserEntity">
        select
        <include refid="userColumnAll"/>
        from user;
    </select>
</mapper>
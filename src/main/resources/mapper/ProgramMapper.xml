<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csl.anarres.mapper.ProgramMapper">

    <resultMap id="programMap" type="com.csl.anarres.entity.ProgramEntity">
        <id column="P_ID" property="programId"></id>
        <id column="LANGUAGE" property="language"></id>
        <id column="CODE" property="code"></id>
        <id column="CREATE_TIME" property="createTime"></id>
        <id column="LAST_MODIFIED_TIME" property="lastModifiedTime"></id>
        <id column="CREATER_ID" property="createrId"></id>
        <id column="CODE_MD5" property="codeMD5"></id>
        <id column="CONTENT_MD5" property="contentMD5"></id>
        <id column="TITLE" property="title"></id>
        <id column="CONTENT" property="content"></id>
        <id column="STATE" property="state"></id>
        <id column="PUBLIC_STATE" property="publicState"></id>
    </resultMap>
    <sql id="programColumnAll">
        P_ID,LANGUAGE,CODE,CREATER_ID,CREATE_TIME,CODE_MD5,OUTPUT
    </sql>
    <select id="findProgram"  resultMap="programMap">
        select
        <include refid="programColumnAll"/>
        from program
        <where>
            LANGUAGE = #{query.language}
            and CODEMD5 = #{query.codeMD5};
        </where>
    </select>
</mapper>